menu "ST STSafe A1xx"

config LIB_STSELIB
    bool "Enable ST STSELib support"
    default n
    help
      Access the STMicroelectronics STSELib.

if LIB_STSELIB

choice
    prompt "STSAFE Chip"
    depends on LIB_STSELIB
    default STSAFE_A120

config STSAFE_A110
    bool "STSAFE A110"

config STSAFE_A120
    bool "STSAFE A120"

endchoice # STSAFE Chip

endif # LIB_STSELIB

endmenu